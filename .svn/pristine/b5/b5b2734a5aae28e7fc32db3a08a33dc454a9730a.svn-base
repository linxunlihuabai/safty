          <el-table-column type="expand">
            <template slot-scope="props">
              <el-form class="demo-table-expand">
                <el-row class="zhou-one">
                  <el-col :span="6" class="zhou-colBorder">
                    <el-form-item label="基地地址：">
                      <span>{{ props.row.address }}</span>
                    </el-form-item>
                  </el-col>
                  <el-col :span="6" class="zhou-colBorder">
