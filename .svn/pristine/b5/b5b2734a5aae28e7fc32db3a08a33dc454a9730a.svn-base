#bmap {
  height: 520px;
}
.demo-table-expand .el-form-item {
  margin-bottom: 0px;
}
.el-row {
  margin-bottom: 0;
}
.el-input--small,.el-select--small,.el-input-number--small{
  width:100%;
}
</style>
