    // 页面底下的地图
    bigEnterpriseMap({ BMap, map }) {
      map.centerAndZoom(new BMap.Point(104.769128, 34.972443), 5)
      // map.setMapType(BMAP_EARTH_MAP)// 设置地图类型为地球模式
      var opts = {
        width: 250, // 信息窗口宽度
